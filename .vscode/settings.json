{
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": false,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "cSpell.words": [
    "donationalerts",
    "genhtml",
    "justinfan",
    "lcov",
    "PRIVMSG",
    "userstate"
  ],
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "[lua]": {
    "editor.defaultFormatter": "trixnz.vscode-lua"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "deno.codeLens.test": true
}
